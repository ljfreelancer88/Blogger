{
    "name": "ljfreelancer88/blogger",
    "type": "library",
    "description": "Awesome Blogger",
    "keywords": [
        "ljfreelancer88",
        "blogger"
    ],
    "homepage": "https://github.com/ljfreelancer88/blogger",
    "license": "MIT",
    "authors": [
        {
            "name": "Jake Pucan",
            "email": "jakepucan@gmail.com",
            "homepage": "https://jakepucan.blogspot.com",
            "role": "Developer"
        }
    ],
	"minimum-stability": "dev",
    "require": {
        "php" : "~7.1"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "ljfreelancer88\\blogger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ljfreelancer88\\blogger\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
